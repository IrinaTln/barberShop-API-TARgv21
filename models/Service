module.exports = (sequelize, Sequelize) => {
    const Service = sequelize.define("service", {
        id_service: {
            type: Sequelize.INTEGER,
            primaryKey: true,
            autoIncrement: true
        },
        ServiceName:{
            type: Sequelize.STRING,
        },
        Price:{
            type: Sequelize.INTEGER,
        }
    })
    
    return Service
}